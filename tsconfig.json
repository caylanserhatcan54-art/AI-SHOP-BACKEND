{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "skipLibCheck": true
  },
  "exclude": [
    "**/node_modules",
    "**/dist",
    "my-app",
    "ai-shop-site",
    "frontend",
    "app",
    "ui"
  ]
}
